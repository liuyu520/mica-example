<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.dreamlu.convention.mapper.ImgItemMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="net.dreamlu.convention.model.ImgItem">
        <id column="id" property="id"/>
        <result column="file_path" property="filePath"/>
        <result column="url" property="url"/>
        <result column="thumbnail" property="thumbnail"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
        <result column="aliass" property="aliass"/>
        <result column="alias2" property="alias2"/>
        <result column="status" property="status"/>
        <result column="groupId" property="groupId"/>
        <result column="type2" property="type2"/>
        <result column="description" property="description"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, file_path, url, thumbnail, create_time, update_time, aliass, alias2, status, groupId, type2, description
    </sql>

</mapper>
