<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.dreamlu.convention.mapper.BabyRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="net.dreamlu.convention.model.BabyRecord">
        <id column="id" property="id"/>
        <result column="day" property="day"/>
        <result column="height" property="height"/>
        <result column="weight" property="weight"/>
        <result column="other" property="other"/>
        <result column="access_day" property="accessDay"/>
        <result column="access_daytime" property="accessDaytime"/>
        <result column="access_type" property="accessType"/>
        <result column="description" property="description"/>
        <result column="attentionMatter" property="attentionMatter"/>
        <result column="foods" property="foods"/>
        <result column="status" property="status"/>
        <result column="updateTime" property="updateTime"/>
        <result column="createTime" property="createTime"/>
        <result column="picturePath" property="picturePath"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, day, height, weight, other, access_day, access_daytime, access_type, description, attentionMatter, foods, status, updateTime, createTime, picturePath
    </sql>

</mapper>
