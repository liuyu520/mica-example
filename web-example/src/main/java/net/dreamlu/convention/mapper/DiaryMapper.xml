<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.dreamlu.convention.mapper.DiaryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="net.dreamlu.convention.model.Diary">
        <id column="id" property="id"/>
        <result column="content" property="content"/>
        <result column="createTime" property="createTime"/>
        <result column="stars" property="stars"/>
        <result column="status" property="status"/>
        <result column="updateTime" property="updateTime"/>
        <result column="userId" property="userId"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, content, createTime, stars, status, updateTime, userId
    </sql>

</mapper>
