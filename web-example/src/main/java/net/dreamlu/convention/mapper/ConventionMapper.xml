<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.dreamlu.convention.mapper.ConventionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="net.dreamlu.convention.model.Convention">
        <id column="id" property="id"/>
        <result column="answer" property="answer"/>
        <result column="pic" property="pic"/>
        <result column="update_time" property="updateTime"/>
        <result column="stars" property="stars"/>
        <result column="status" property="status"/>
        <result column="create_time" property="createTime"/>
        <result column="testId" property="testId"/>
        <result column="hasEncrypted" property="hasEncrypted"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, answer, pic, update_time, stars, status, create_time, testId, hasEncrypted
    </sql>

</mapper>
