<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.dreamlu.convention.mapper.ContractMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="net.dreamlu.convention.model.Contract">
        <id column="id" property="id"/>
        <result column="workTitle" property="workTitle"/>
        <result column="birthAddress" property="birthAddress"/>
        <result column="currentAddress" property="currentAddress"/>
        <result column="age" property="age"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, workTitle, birthAddress, currentAddress, age
    </sql>

</mapper>
