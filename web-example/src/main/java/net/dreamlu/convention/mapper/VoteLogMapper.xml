<mapper namespace="net.dreamlu.convention.mapper.VoteLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="net.dreamlu.convention.model.VoteLog">
        <id column="id" property="id"/>
